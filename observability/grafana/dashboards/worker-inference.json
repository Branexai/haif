{
  "id": null,
  "title": "Worker Inference",
  "tags": ["tether"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Inference requests per second",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "sum(rate(infer_requests_total[1m])) by (model)", "legendFormat": "{{model}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Inference failures per second",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "sum(rate(infer_failures_total[1m])) by (model)", "legendFormat": "{{model}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Inference duration P95",
      "gridPos": {"x": 0, "y": 8, "w": 24, "h": 8},
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(infer_duration_seconds_bucket[5m])) by (le, model))", "legendFormat": "{{model}}"}
      ]
    }
  ],
  "templating": {"list": []}
}